= form_for @contact_list do |f|
  - if @contact_list.errors.any?
    #error_explanation
      %h2= "#{pluralize(@contact_list.errors.count, "error")} prohibited this contact_list from being saved:"
      %ul
        - @contact_list.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :user_id
    = f.number_field :user_id
  .field
    = f.label :name
    = f.text_field :name
  .actions
    = f.submit 'Save'
