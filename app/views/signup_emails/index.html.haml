%h1 Listing signup_emails

%table
  %tr
    %th Email
    %th Description
    %th
    %th
    %th

  - @signup_emails.each do |signup_email|
    %tr
      %td= signup_email.email
      %td= signup_email.description
      %td= link_to 'Show', signup_email
      %td= link_to 'Edit', edit_signup_email_path(signup_email)
      %td= link_to 'Destroy', signup_email, :method => :delete, :data => { :confirm => 'Are you sure?' }

%br

= link_to 'New Signup email', new_signup_email_path
